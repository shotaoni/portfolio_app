[
  {
    "name": "front-container",
    "image": "180680930541.dkr.ecr.ap-northeast-1.amazonaws.com/tante-front:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "front",
        "awslogs-group": "/ecs/front"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "command": [
      "yarn", "start"
    ],
    "environment": [
      {
        "name": "NODE_ENV",
        "value": "production"
      },
      {
        "name": "WORKDIR",
        "value": "app"
      },
      {
        "name": "HOST",
        "value": "0.0.0.0"
      },
      {
        "name": "PORT",
        "value": "3000"
      }
    ],
    "secrets": [
      {
        "name": "API_ENDPOINT",
        "valueFrom": "api-endpoint"
      },
      {
        "name": "API_KEY",
        "valueFrom": "api-key"
      },
      {
        "name": "AUTH_DOMAIN",
        "valueFrom": "auth-domain"
      },
      {
        "name": "DATABASE_URL",
        "valueFrom": "database-url"
      },
      {
        "name": "PROJECT_ID",
        "valueFrom": "project-id"
      },
      {
        "name": "STORAGE_BUCKET",
        "valueFrom": "storage-bucket"
      },
      {
        "name": "MESSAGE_SENDER_ID",
        "valueFrom": "message-sender-id"
      }
    ]
  }
]